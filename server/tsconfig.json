
{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
      "typeRoots": ["./@types", "./node_modules/@types"],
    "outDir": "dist"
  },
  "include": ["./**/*.ts","./@types/**/*.d.ts"],
  "exclude": ["node_modules"]
}
